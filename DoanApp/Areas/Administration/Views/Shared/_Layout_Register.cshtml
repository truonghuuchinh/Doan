<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/Client/fonts/fontawesome/css/all.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/material-design-iconic-font/2.2.0/css/material-design-iconic-font.min.css">
    <link href="~/css/alert_css.css" rel="stylesheet" />
    <link href="~/css/toastifier.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="/Administration/assets/layoutadmin/css/theme.css">
    <link rel="stylesheet" href="/Administration/assets/layoutadmin/css/style.css">
    @RenderSection("Style",false)
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>
<body class="animsition">
    <div>
        <div class="page-wrapper">
            <div class="page-content--bge5">
                <div class="container">
                    <div class="login-wrap">
                        <div class="login-content">
                            <div class="login-logo">
                                <a href="#">
                                    <img src="/Administration/assets/layoutadmin/img/logoadmin.png" alt="CoolAdmin">
                                </a>
                            </div>
                            <div class="login-form">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
      
    </div>
    <div class="img-loading">
        <div>
            <span class="spinner-border spinner-border-sm"></span>
            Loading..
        </div>
    </div>
    @RenderSection("Scripts",false)
</body>
</html>
